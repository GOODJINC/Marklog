{
  "manifest_version": 3,
  "name": "Marklog - New Tab Bookmarks",
  "version": "1.2.2",
  "description": "Bookmark dashboard for new tab - View all folders and sites at a glance",
  "default_locale": "en",
  "homepage_url": "https://github.com/GOODJINC/Marklog",

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": ["storage"],

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
