{
  "manifest_version": 3,
  "name": "Marklog",
  "version": "1.0.0",
  "description": "한눈에 보는 북마크 대시보드 - 폴더와 사이트를 와이드 화면에서 한번에 확인",
  
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  
  "permissions": [
    "storage"
  ],
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
